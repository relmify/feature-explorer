{
  "name": "feature-explorer",
  "displayName": "FeatureExplorer",
  "description": "A tree view for your gherkin features and scenarios",
  "repository": "https://github.com/relmify/gherkin-explorer",
  "publisher": "relmify",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "gherkin",
    "cucumber",
    "bdd",
    "feature",
    "specflow",
    "scenario",
    "behave",
    "behavior"
  ],
  "activationEvents": [
    "onCommand:featureExplorer.refresh",
    "onView:featureExplorer.view"
  ],
  "main": "./out/application/vscodeExtension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "featureExplorer.view",
          "name": "Feature Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "featureExplorer.refresh",
        "title": "Feature Explorer: Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm run-all check-lint check-prettier compile testall docs",
    "compile": "tsc -p ./",
    "clean": "rimraf out",
    "watch": "tsc -watch -p ./",
    "check-lint": "eslint ./application --ext .ts --ext .js",
    "fix-lint": "eslint ./application --ext .ts --ext .js --fix",
    "check-prettier": "prettier --check \"{application,jest}/**/*.{ts,js}\"",
    "docs": "typedoc",
    "testall": "npm-run-all test:*",
    "jest": "jest",
    "jestwatch": "jest --watch",
    "test": "jest",
    "test:unit": "jest",
    "test:integration": "node ./out/application/test/integration/jestTestRunner.js",
    "test:feature": "cucumber-js  -p default --format summary",
    "unit-test": "npm-run-all compile test:unit",
    "integration-test": "npm-run-all compile test:integration",
    "feature-test": "npm-run-all compile test:feature"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/ramda": "^0.26.44",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "cucumber": "^6.0.5",
    "cucumber-tsflow": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-unused-imports": "^0.1.2",
    "glob": "^7.1.6",
    "jest": "^25.2.7",
    "jest-extended": "^0.11.5",
    "jest-watch-toggle-config": "^1.0.2",
    "marked": "^0.8.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.11",
    "typedoc-neo-theme": "^1.0.7",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "fp-ts": "^2.5.3",
    "gherkin": "^9.0.0",
    "globby": "^11.0.0",
    "io-ts": "^2.1.2",
    "io-ts-types": "^0.5.6",
    "monocle-ts": "^2.1.0",
    "rxjs": "^6.5.4"
  }
}
